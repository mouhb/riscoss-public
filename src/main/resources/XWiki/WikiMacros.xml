#set($wikiMacroDocuments = $services.query.xwql("from doc.object(XWiki.WikiMacroClass) as macros").execute())
#if($wikiMacroDocuments.size() > 0)
|=$services.localization.render('xe.wikimacrobridge.macroName') {{html}}&lt;tt>($services.localization.render('xe.wikimacrobridge.macroId'))&lt;/tt>{{/html}}|=$services.localization.render('xe.wikimacrobridge.macroDescription')|=$services.localization.render('xe.wikimacrobridge.macroVisibility')|=$services.localization.render('xe.wikimacrobridge.macroPage')
  #foreach($macroDocumentName in $wikiMacroDocuments)
    #set($macroDocument = $xwiki.getDocument($macroDocumentName))
    #set($macroObject = $macroDocument.getObject("XWiki.WikiMacroClass"))
|$macroObject.get('name') {{html}}&lt;tt>($macroObject.id)&lt;/tt>{{/html}}|$macroObject.description|$macroObject.visibility|[[$macroDocument.getDisplayTitle()>>$macroDocument.fullName]]
  #end
#else
{{warning}}$services.localization.render('xe.wikimacrobridge.noWikiMacro'){{/warning}}
#end
